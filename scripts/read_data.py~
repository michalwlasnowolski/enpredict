    def read_data(self, sequence):
        #data =[]
        kmers=filename.split('\n')[:-1]
        data_dict={}
        seq=sequence.split('\n')[:-1]
        #print sequence.split('\n')[:-1]
        for i in xrange(len(kmers)):
#            data.append(map(float, kmers[i].split()))
            data_dict[str(i)]=map(float, kmers[i].split())

        gc = []
        frames_len=len(sequence)

        for i in xrange(frames_len):
            if len(sequence[i]) > 0:
                gc.append(float(sequence[i].count("G") + sequence[i].count("C") + sequence[i].count("g") + sequence[i].count("c"))/len(sequence[i]))
            else:
                gc.append(0.5)
	        #print "gc robi %d z %d" % (i, frames_len)
            data_dict["GC-content"] = gc
            #print gc



        return data_dict["GC-content"]
