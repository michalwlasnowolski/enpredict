



function downloadWigfile(csrf_token) {


    

    
    

    var chkBoxArray = [];
    $('.chkbox:checked').each(function() {
            chkBoxArray.push($(this).val());
        });

    window.alert(chkBoxArray);


$.ajax({
        method: "POST",
        url: "download_wigfile",
        data: {select_box: chkBoxArray, csrfmiddlewaretoken: csrf_token },
		success:function(data) {
			processDownload(data); 
		}
    //}).done(function( msg ) {

    //});
   })

}

